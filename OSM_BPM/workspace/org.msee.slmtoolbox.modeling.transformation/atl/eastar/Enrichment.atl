rule Resource2Lane{ --check
	from 
		s: EA!EaResource
	using{
		laneName: String = s.name;		
	}
	to
		a: BPMN!Lane(
		id <- s.id,
		name <- s.name,
		flowNodeRefs <-a.flowNodeRefs->including(thisModule.createFlowNodeRefs(s.id))
		)	
	do{	
		--thisModule.bpmnOrgLanes<-thisModule.bpmnOrgLanes->including(a);	
		if(not thisModule.bpmnDefaultLanes->includes(laneName)){
			thisModule.getNamedLaneSet('VirtualWorldLaneSet')->first().lanes<-thisModule.getNamedLaneSet('VirtualWorldLaneSet')->first().lanes->including(a);
		}		
		thisModule.bpmnDefaultLanes<-Set{thisModule.bpmnDefaultLanes,laneName};	
	}
}

