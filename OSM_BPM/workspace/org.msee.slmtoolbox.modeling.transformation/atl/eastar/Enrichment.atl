entrypoint rule init() {
	to 
	
		a: BPMN!Collaboration (
			name <- 'collaboration',
			id <- 'collaboration_1'
		),
		real: BPMN!LaneSet(
			name <- 'VirtualWorldLaneSet',
			id <- 'VirtualWorldLaneSet',
			lanes <- thisModule.bpmnDefaultLanes
	
		),
		osmose: BPMN!LaneSet(
			name <- 'OsmoseWorldLaneSet',
			id <- 'OsmoseLaneSet'
	
		),
		digital: BPMN!LaneSet(
			name <- 'DigitalWorldLaneSet',
			id <- 'DigitalWorldLaneSet'
		),
		c: BPMN!Process(
			name <- 'ProcessToStoreFlowElements',
			id <- 'processStore'
)
	do {
		thisModule.bpmnDefaultLanes <-Set{};
		thisModule.bpmnProcess<-Set{};
		thisModule.orgChildLaneSet <-Set{};
		thisModule.bpmnOrgLanes <-Set{};
		thisModule.bpmnFlowElements <-Set{};
		
		thisModule.membraneFlowElements <-Set{};
		thisModule.digitalFlowElements <-Set{};
		thisModule.MembraneStartingTask();
		
		thisModule.MembraneElements();
		thisModule.EnrichmentElements();
		thisModule.MembraneSequenceFlows();
		thisModule.AugmentationSequenceFlows();
		
		
		thisModule.collaborations <-a ;
		thisModule.laneSet <-Set{real,osmose,digital};

	}
}

-- transform an ExtendedActigram element to a Definition element
rule ExtendedActigramToDefinition {
	from
		s: EA!EaModel
	to
		a: BPMN!Definitions (
			id <- s.name,
			name <- 'enrichment',
			targetNamespace <- 'http://www.jboss.org/drools',
			expressionLanguage <- 'http://www.mvel.org/2.0',
			typeLanguage <- 'http://www.java.com/javaTypes',
			rootElements <- Set{s.process.flowElements -> select(e | e.
					oclIsTypeOf(EA!EaExternalConnector) or e.
					oclIsTypeOf(EA!EaInternalConnector)) -> collect(e | thisModule.
					InExternalConnector2Process(e)).
					insertAt(1, thisModule.ProcessToProcess(s.process)).
					append(thisModule.collaborations), thisModule.bpmnProcess}
		)
	do {
		let s: String =
				''
			in
				'definition'.println(); 
	}
}

