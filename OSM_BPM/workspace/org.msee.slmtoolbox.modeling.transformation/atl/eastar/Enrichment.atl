	}		
}



rule CreateLane (laneName: OclAny, LaneSet : OclAny) { --check
to
			lane : BPMN!Lane (
			name <- laneName,
			id <- laneName		
		)
	do{
		LaneSet.lanes <- LaneSet.lanes->including(lane);
		if(laneName = 'Membrane'){
			lane.flowNodeRefs<-thisModule.membraneFlowElements->select(x|not x.oclIsTypeOf(BPMN!SequenceFlow));
		}
		if(laneName = 'Enrichment'){
			lane.flowNodeRefs<-thisModule.digitalFlowElements->select(x|not x.oclIsTypeOf(BPMN!SequenceFlow));
		}
		
	}
}

